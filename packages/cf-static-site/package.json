    "@fmtk/acm-auto-cert": "^0.5.0",
    "@fmtk/custom-resources-commons": "^0.4.1",
    "@fmtk/custom-resources-commons-cdk": "^0.4.2",
    "@fmtk/s3-empty-bucket": "^0.2.3",
    "@fmtk/s3-unpack-asset": "^0.3.1",
    "@fmtk/validation": "^3.4.1"
  },
  "devDependencies": {
    "@fmtk/aws-custom-resource": "^0.3.1",
    "aws-sdk": "^2.548.0"
